<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="tests">
   <insert id="register" parameterType="memberVo">
          INSERT INTO TEST (id)
          VALUES (#{id})
   </insert>


    <select id="getmemberList" resultType="memberVo">
        SELECT * FROM TEST
    </select>

</mapper>